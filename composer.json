{
	"name": "stoic/stoic",
	"description": "Stoic framework Core component",
	"version": "1.0.2",
	"homepage": "https://stoic-framework.com",
	"autoload": {
		"psr-0": {
			"Stoic\\Chain\\": "./Chain",
			"Stoic\\Log\\":  "./Log",
			"Stoic\\Utilities\\":  "./Utilities"
		},
